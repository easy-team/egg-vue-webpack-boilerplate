{
  "name": "egg-vue-webpack-boilerplate",
  "version": "3.0.2",
  "description": "基于easywebpack-vue和egg-view-vue(ssr)插件的工程骨架项目",
  "scripts": {
    "build": "cross-env easywebpack build prod",
    "build-dev": "cross-env easywebpack build dev",
    "build-test": "cross-env easywebpack build test",
    "start": "node index.js",
    "start-test": "cross-env EGG_SERVER_ENV=test node index.js",
    "start-prod": "cross-env EGG_SERVER_ENV=prod NODE_ENV=production npm start",
    "kill": "pkill --signal SIGINT egg-vue-webpack-boilerplate",
    "kill-mac": "kill -9 $(lsof -i:7001 |grep 'node'|awk '{print $2}')",
    "kill-win": "taskkill -PID $(netstat -ano | findstr 7001|awk '{print $5}') -F",
    "restart": "npm run kill-port && npm run start",
    "lint": "eslint .",
    "fix": "eslint --fix ."
  },
  "dependencies": {
    "axios": "^0.16.2",
    "co": "^4.6.0",
    "co-wait": "^0.0.0",
    "core-js": "^2.4.0",
    "egg": "^1.4.0",
    "egg-logger": "^1.5.0",
    "egg-validate": "^1.0.0",
    "egg-view-vue": "^1.0.0",
    "egg-view-vue-ssr": "^2.1.1",
    "element-ui": "^1.4.1",
    "extend": "~3.0.0",
    "lodash": "^4.17.4",
    "mint-ui": "^2.2.9",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.17.1",
    "vue": "^2.4.2",
    "vue-hot-reload-api": "^2.1.0",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.4",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "autod-egg": "^1.0.0",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.0",
    "directory-named-webpack-plugin": "^2.2.3",
    "easywebpack-vue": "^3.2.0",
    "egg-cors": "^1.1.0",
    "egg-logview": "^1.0.0",
    "egg-webpack": "^3.0.0",
    "egg-webpack-vue": "^1.0.0",
    "eslint": "^4.3.0",
    "eslint-config-egg": "^5.0.0",
    "eslint-plugin-vue": "^2.0.1",
    "html-webpack-plugin": "^2.30.1",
    "imagemin-webpack-plugin": "^1.5.2",
    "ip": "^1.1.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "npm-install-webpack-plugin": "^4.0.5",
    "postcss-loader": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "sass-loader": "^6.0.6",
    "vconsole": "^2.5.2",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "ci": {
    "version": "6, 7, 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubcarl/egg-vue-webpack-boilerplate.git"
  },
  "author": "hubcarl@126.com",
  "license": "MIT",
  "homepage": "https://github.com/hubcarl/egg-vue-webpack-boilerplate"
}
